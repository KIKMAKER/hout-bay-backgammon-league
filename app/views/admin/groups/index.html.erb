<h2>All Groups</h2>
<div class="container">
  <table class="custom-table table">
    <thead>
    </thead>
    <tbody>
      <% @groups.each do |group| %>
        <tr>
          <td class="custom-table"><%= link_to group.title, admin_group_path(group) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <div class="container text-center">
    <%= link_to 'Create Group', new_admin_group_path, class: "btn white-button mt-3" %>
  </div>
</div>
